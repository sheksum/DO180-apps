apiVersion: apps/v1
kind: Deployment
metadata:
  name: registry-pull-test
spec:
  replicas: 1
  selector:
    matchLabels:
      app: registry-pull-test
  template:
    metadata:
      labels:
        app: registry-pull-test
    spec:
      containers:
      - name: nginx
        image: plnx-ai-registry.calix.local/nginx:latest
        imagePullPolicy: Always
        ports:
        - containerPort: 80