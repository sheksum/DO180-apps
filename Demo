hsuma@pln-aidev02:~/external-secret-operator$ cat clustersecretstore.yaml 
apiVersion: external-secrets.io/v1
kind: ClusterSecretStore
metadata:
  name: vault-backend
spec:
  provider:
    vault:
      server: "https://plnx-vault.calix.local:8200"
      path: "secret/data"      # KV V2 path
      version: "v2"
      auth:
        tokenSecretRef:
          name: vault-token
          namespace: vault-poc
          key: token
      caBundle: |
        
